%h1 Listing event_feedbacks

%table
  %thead
    %tr
      %th Comment


  %tbody
    - @event_feedbacks.each do |event_feedback|
      %tr
        %td= event_feedback.comment
        %td= link_to 'Show', event_feedback
        %td= link_to 'Edit', edit_event_feedback_path(event_feedback)
        %td= link_to 'Destroy', event_feedback, method: :delete, data: { confirm: 'Are you sure?' }

%br

= link_to 'New Event feedback', new_event_feedback_path
